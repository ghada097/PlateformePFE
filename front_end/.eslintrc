{
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"

    },
    "extends": ["eslint:recommended", "eslint-config-prettier"],
    "rules": {
      "no-console": "off",
      "quotes": ["off", "double"],
      "semi": "off",
      "no-undef": "off",
      "no-unused-vars": "off",
      "capitalized-comments":"off"
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "parser": "babel-eslint"
    

  }